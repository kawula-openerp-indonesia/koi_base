<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="tree_base_agama" model="ir.ui.view">
            <field name="name">tree Agama</field>
            <field name="model">base.agama</field>
            <field name="arch" type="xml">
                <tree string="Agama">
					<field name="kode"/>
                    <field name="name" select="1"/>
                </tree>
            </field>
        </record>
		<record id="form_base_agama" model="ir.ui.view">
            <field name="name">form Agama</field>
            <field name="model">base.agama</field>
            <field name="arch" type="xml">
                <form string="Agama">
				    <field name="kode"/>
                    <field name="name" />
                </form>
            </field>
        </record>



    </data>
</openerp>
