<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="form_base_jenisKelamin" model="ir.ui.view">
            <field name="name">form Jenis Kelamin</field>
            <field name="model">base.jenis_kelamin</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Jenis Kelamin">
					<field name="kode"/>
                    <field name="name" select="1"/>
                </form>
            </field>
        </record>

		<record id="tree_base_jenisKelamin" model="ir.ui.view">
            <field name="name">tree Jenis Kelamin</field>
            <field name="model">base.jenis_kelamin</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Jenis Kelamin">
					<field name="kode"/>
                    <field name="name" />
                </tree>
            </field>
        </record>

    </data>
</openerp>
